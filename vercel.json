{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/blog",
      "destination": "/blogs.html"
    },
    {
      "source": "/careers",
      "destination": "/career.html"
    },
    {
      "source": "/case-studies",
      "destination": "/case studies.html"
    },
    {
      "source": "/get-started",
      "destination": "/get started.html"
    },
    {
      "source": "/help-center",
      "destination": "/help center.html"
    },
    {
      "source": "/powerups",
      "destination": "/powerups.html"
    },
    {
      "source": "/ai-helpers",
      "destination": "/what is an AI helper.html"
    },
    {
      "source": "/pricing",
      "destination": "/get started.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
